#include <xs1.h>

in port iButton = XS1_PORT_32A;
out port oLed = XS1_PORT_1A;

int main(){
    unsigned value;
    while(1){
        iButton :> value;
        if(value){
            //todo illuminate the led
            oLed = 1;
        }

        else{
            oLed = 0;
        }
        //turn off led

    }
}
